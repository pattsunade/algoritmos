   c = a+b;
   console.log('c is', c);
   return c;
 }
 x = op(2,3) + op(3,5);
 console.log('x is', x);

 salida: c is 5
        c is 8
        x is 13

ejercicio 6//
 function op(a,b) {
   c = a+b;
   console.log('c is', c);
   return c;
 }
 x = op(2,3) + op(3,op(2,1)) + op(op(2,1), op(2,3));
 console.log('x is', x );
 salida: c is 5
        c is 3
        c is 6
        c is 3
        c is 5
        c is 8
        x is 19
