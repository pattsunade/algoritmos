ejercicio 3//
function a(n){
   console.log('n is', n);
   return n+15;
 }
 x = a(3);
 console.log('x is', x);

 salida: n is 3
        x is 18

ejercicio 4//
function a(n) {
   console.log('n is', n);
   y = n*2;
   return y;
 }
 x = a(3) + a(5);
 console.log('x is', x);
  
  saida: n is 3
        n is 5
        x is 16

ejercicio 5//

